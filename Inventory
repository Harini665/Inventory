const icecream = {
    chocolate: {
        flavor: "chocolate",
        price: 200,
        quantity: 50
    },
    mango: {
        flavor: "mango",
        price: 180,
        quantity: 40
    },
    strawberry: {
        flavor: "strawberry",
        price: 220,
        quantity: 30
    }
};
icecream.vennila= {flavor: "vennila",price: 250,quantity: 45};
console.log(icecream);
let totalquantity=icecream.chocolate.quantity+icecream.mango.quantity+icecream.strawberry.quantity+icecream.vennila.quantity;
console.log(totalquantity);
let totalprice=icecream.chocolate.price+icecream.mango.price+icecream.strawberry.price+icecream.vennila.price;
console.log(totalprice);
